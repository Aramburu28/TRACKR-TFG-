<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Points.TestPage"
             Title="TestPage">
    <VerticalStackLayout>
        <Entry
          x:Name="btnad"
          Placeholder="Enter username"
          Completed="btnad_Clicked"/>
        <Label x:Name="Result"/>
        <Button
            x:Name="btnsee"
            Text="View"
            Clicked="btnsee_Clicked"/>
        <Label x:Name="List"/>
    </VerticalStackLayout>
</ContentPage>